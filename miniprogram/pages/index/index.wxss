.category {
  display: flex;
  justify-content: space-around;
  width: 100%;
  height: 190rpx;
  margin-top: 10rpx;
  background-color: white;
}

.navigator {
  flex-grow: 1;
  width: 50rpx;
}

.img {
  margin-top: 18rpx;
  width: 100%;
  height: 70%;
  text-align: center;
}

.img image {
  margin: auto 0rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-top: 10rpx;
}

.weui-media-box__info__meta {
  color: #748AA1;
  font-size: 12px;
}

.weui-media-box__info__meta_extra {
  color: black;
  font-size: 12px;
}

.weui-search-bar__cancel-btn {
  color: rgb(64, 167, 69);
}

.swiper {
  width: 100%;
  height: 350rpx;
}

.slide-img {
  width: 100%;
  height: 100%;
}

.rebang {
  display: flex;
  padding-top: 30rpx;
  height: 60rpx;
}

.rebang_hd {
  padding-left: 16px;
  color: red;
  font-size: 40rpx;
  width: 180rpx;
  display: flex;
}

.rebang_hd image {
  width: 100%;
  height: 100%;
}

.swiper_container {
  padding-left: 16px;
  height: 60rpx;
  width: 100%;
  text-align: left;
}

.swiper_item {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.zhiding_image {
  height: 50rpx;
  width: 100rpx;
}